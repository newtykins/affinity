{
	"name": "affinity-osu",
	"version": "1.0.0",
	"description": "The simplest osu! toolkit around (:",
	"main": "dist/index.js",
	"scripts": {
		"commit": "cz",
		"build": "gulp",
		"lint": "prettier src --write",
		"test": "jest --setupFiles dotenv/config --verbose --forceExit",
		"test:watch": "npm run test --watchAll",
		"examples:pp": "ts-node example/ppLeaderboard.ts",
		"examples:replay": "ts-node example/downloadReplay.ts",
		"docs:deps": "cd ./docs && pnpm install",
		"docs:start": "cd ./docs && pnpm start",
		"docs:build": "cd ./docs && pnpm build",
		"docs:serve": "cd ./docs && pnpm serve",
		"prepare": "npx only-allow pnpm && pnpm docs:deps",
		"phobia": "bundle-phobia -p package.json"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.26.0",
		"lodash.camelcase": "^4.3.0",
		"ojsama": "^2.2.0"
	},
	"devDependencies": {
		"@types/gulp": "^4.0.9",
		"@types/gulp-uglify": "^3.0.7",
		"@types/inquirer": "^8.2.0",
		"@types/jest": "^27.4.0",
		"@types/lodash.camelcase": "^4.3.6",
		"@types/node": "^17.0.18",
		"bundle-phobia-cli": "^0.14.13",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.0",
		"dts-minify": "^0.3.0",
		"glob": "^7.2.0",
		"gulp": "^4.0.2",
		"gulp-cli": "^2.3.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"gulp-uglify": "^3.0.2",
		"husky": "^7.0.4",
		"inquirer": "^8.2.1",
		"jest": "^27.5.1",
		"only-allow": "^1.1.0",
		"pnpm": "^6.32.2",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.5.0",
		"ts-transform-paths": "^2.0.3",
		"ttypescript": "^1.5.13",
		"typescript": "^4.5.5"
	}
}
